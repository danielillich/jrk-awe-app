{
  "version": 2,
  "functions": {
    "api/data.js": {
      "runtime": "@vercel/node"
    },
    "api/demo-data.js": {
      "runtime": "@vercel/node"
    },
    "api/checkin/[id].js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
